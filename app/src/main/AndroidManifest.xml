<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.android.theshelf"
    tools:ignore="WrongManifestParent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning,RtlEnabled,WrongManifestParent">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Admin_Login"></activity>
        <activity android:name=".Admin_Menu"></activity>
        <activity android:name=".Add_Student"></activity>
        <activity android:name=".Add_books"></activity>
        <activity android:name=".issue_books"></activity>
        <activity android:name=".Issued_books_list"></activity>
        <activity android:name=".Books_list"></activity>
        <activity android:name="Return_books"></activity>

    </application>

</manifest>